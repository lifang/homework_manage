<% if @question_types.present? && @question_types.class == Array %>
  <% @question_types.each do  |quetion_type| %>
  <div class="sb_item_list">
    <div class="item_name">
      <div class="<%= questions_img_class(quetion_type) %>"></div>
      <div ></div>
      <h2><%= Question::TYPES_NAME[quetion_type.to_i]  %></h2>
      <p>平均正确率：68% </p>
      <p>平均用时：13.11秒</p>
    </div>
    <div class="item_content">
      <ul>
        <li class="<%= correct_rate_color 100 %>"><span>1</span><a href="javascript:void(0);" class="tooltip_title" title="点击查看">标签</a>100%</li>
