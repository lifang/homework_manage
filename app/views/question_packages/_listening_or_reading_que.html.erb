<div class="assignment_body_list">
  <div class="ab_list_title">
    <h1>未命名</h1>
    <p><%= Question::TYPES_NAME[@question.types] %>/ <%= @user.name %>
      / <%= @question.created_at.strftime("%Y-%m-%d") %>/ 参考时间：<label>未设置</label></p>
    <ul class="ab_func">
      <li><a href="#" class="clock_icon tooltip_html">设定时间</a></li>
      <li><a href="#" class="save_icon tooltip_html">保存</a></li>
      <li><a href="#" class="share_icon tooltip_html">分享</a></li>
      <li><a href="#" class="delete_icon tooltip_html">删除</a></li>
    </ul>
    <input type="text" value="<%= @question.types.to_i %>" class="question_types">
  </div>
  <div class="ab_list_box" style="display: none;">
    <div class="ab_article">
      <div class="questions_item">
        <div class="q_topic q_willCompile">
          <div class="speedQuestions">
            <ul>
              <li><a href="#" class="voice tooltip_title" title="点击上传音频"></a></li>
              <li class="qt_input"><input name="" type="text" value="在此输入文字"></li>
            </ul>
          </div>
        </div>
        <div class="qt_icon">
          <a href="#" class="save tooltip_html">保存</a>
          <a href="#" class="tag tooltip_html">标签</a>
        </div>
        <div class="tag_ul">
          <ul>
          </ul>
          <input type="text" class="tags_id" value="">
        </div>
      </div>
    </div>
  </div>
</div>