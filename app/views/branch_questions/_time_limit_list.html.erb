<% from ||="" %>
<div class="ab_list_box" style="display: none;">
  <div class="ab_article">
    <% branch_questions.each do |bq| %>
      <div class="questions_item">
        <div class="q_topic">
          <div class="speedQuestions">
            <ul>
              <li><a href="javascript:void(0)" class="<%= bq[:answer] == "true" ? "true" : "false"  %>">T</a></li>
              <li class="show_text"><p><%= bq.content  %></p></li>
              <li><a href="javascript:void(0)" class="<%= bq[:answer] == "true" ? "false" : "true"  %>">F</a></li>
            </ul>
          </div>
        </div>
        
        <% if branch_tags[bq.id].present? %>
          <div class="tag_ul">
            <ul>
              <% branch_tags[bq.id].each do |tag| %>
                <li><p><%= tag.name %></p></li>
              <% end %>
            </ul>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
