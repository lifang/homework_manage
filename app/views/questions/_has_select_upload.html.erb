<% if @type=="voice" %>
  <li onclick='playAudio(this)'>
    <img src='/assets/voiceing.jpg'/>
    <audio  controls="controls" style="display: none;">
      <source type="audio/mpeg" src="<%= @url %>">
        Your browser does not support the audio element.
    </audio>
  </li>
<% end %>
<% if @type=="photo" %>
  <img src="<%= @url %>" style="width:86px;height:86px;"/>
<% end %>