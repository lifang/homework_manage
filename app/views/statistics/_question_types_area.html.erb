        <li class="<%= correct_rate_color 70 %>"><span>4</span><a href="javascript:void(0);" class="tooltip_title" title="点击查看">标签</a>100%</li>
        <li class="<%= correct_rate_color 60 %>"><span>5</span><a href="javascript:void(0);" class="tooltip_title" title="点击查看">标签</a>100%</li>
        <li class="<%= correct_rate_color 50 %>"><span>6</span><a href="javascript:void(0);" class="tooltip_title" title="点击查看">标签</a>100%</li>
        <li class="<%= correct_rate_color 40 %>"><span>7</span><a href="javascript:void(0);" class="tooltip_title" title="点击查看">标签</a>100%</li>
        <li class="<%= correct_rate_color 30 %>"><span>8</span><a href="javascript:void(0);" class="tooltip_title" title="点击查看">标签</a>100%</li>
