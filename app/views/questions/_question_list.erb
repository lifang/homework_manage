<% if @questions %>
  <% @questions.each do |question| %>
    <div class="ab_list_title" onclick="show_branch_question(this,<%= question.question_package_id %>,<%= question.id %>,<%= question.types %>)">
      <h1><%= question.name %></h1>
      <p>选择 / 李芳 / 2014-02-12 / 参考时间：<%= question.questions_time %></p>
      <ul class="ab_func">
        <li><a href="javascript:void(0)" class="clock_icon tooltip_html">设定时间</a></li>
        <li><a href="javascript:void(0)" class="save_icon tooltip_html">保存</a></li>
        <li><a href="javascript:void(0)" class="share_icon tooltip_html">分享</a></li>
        <li><a href="javascript:void(0)" class="delete_icon tooltip_html">删除</a></li>
      </ul>
    </div>
    <div class="ab_list_box" style="display: none;">
    </div>
  <% end %>
<% end %>