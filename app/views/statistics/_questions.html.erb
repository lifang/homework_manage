  <a href="javascript:void(0);" class="close">close</a>
  <div class="tab_head"><%= Question::TYPES_NAME[@origin_questions[:types].to_i] %></div>
  <div class="tab_body tab_questions clearAfter">
  <% @origin_questions[:questions].each do |question|%>
    <% case question[:types] %>
        <% when Question::TYPES[:LISTENING] then %>
                <%= render "statistics/que_listening", {:question => question} %>
        <% when Question::TYPES[:READING] then%>
            <%= render "statistics/que_reading", {:question => question} %>
        <% when Question::TYPES[:TIME_LIMIT] then%>
            <%= render "statistics/que_time_limit", {:question => question} %>
        <% when Question::TYPES[:SELECTING] then%>
            <%= render "statistics/que_selecting", {:question => question} %>
        <% when Question::TYPES[:LINING] then%>
            <%= render "statistics/que_lining", {:question => question} %>
        <% when Question::TYPES[:CLOZE] then%>
            <%= render "statistics/que_cloze", {:question => question} %>
        <% when Question::TYPES[:SORT] then%>
            <%= render "statistics/que_sort", {:question => question} %>
    <% end %>
<% end %>
</div>