<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <thead>
  <tr>
    <th>作业包创建时间</th>
    <th>备注</th>
    <th>预览</th>
    <th>编辑</th>
    <th>删除</th>
    <th>截止时间</th>
  </tr>
  </thead>
  <tbody>
  <% @publish_question_packages.each do |p| %>
      <% if p.publish_question_package_id.nil?  %>
          <tr>
            <td><%= p.created_at.to_s[0,19] %></td>
            <td><p class="tooltip"><%= p.name %></p></td>
            <td><a href="/question_packages/<%= p.question_package_id %>" class="td_view">预览</a></td>
            <td><a href="/question_packages/<%= p.question_package_id %>/questions" class="td_write">编辑</a></td>
            <td><a href="#" class="td_trash">删除</a></td>
            <td><a href="#" class="td_timer" onclick="show_publish_task_panel(<%= p.question_package_id %>)">时间</a></td>
          </tr>
      <% else %>
          <tr>
            <td><%= p.created_at.to_s[0,19] %></td>
            <td><p class="tooltip"><%= p.name %></p></td>
            <td><a href="/question_packages/<%= p.question_package_id %>" class="td_view">预览</a></td>
            <td><a href="/question_packages/<%= p.question_package_id %>/questions" class="td_write">编辑</a></td>
            <td><a href="#" class="td_trash" onclick="delete_packages(<%= p.publish_question_package_id  %>,<%= @school_class.id %>)">删除</a></td>
            <td><%= p.end_time.to_s[0,19] %></td>
          </tr>
      <% end %>
  <% end %>
  </tbody>
</table>
<%= will_paginate @publish_question_packages,{:previous_label => "上一页", :next_label => "下一页"} %>

