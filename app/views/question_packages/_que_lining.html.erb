<% @question[type].each do |que| %>
    <div class="assignment_body_list">
      <div class="ab_list_title">
        <h1><%= que[:name]   %></h1>
        <p><%= Question::TYPES_NAME[que[:types]]  %> / <%= @user.name %> / <%= que[:created_at] %>> / 参考时间：<%= que[:questions_time] %>秒</p>
        <ul class="ab_func">
          <li><a href="#" class="clock_icon tooltip_html">设定时间</a></li>
          <li><a href="#" class="save_icon tooltip_html">保存</a></li>
          <li><a href="#" class="share_icon tooltip_html">分享</a></li>
          <li><a href="#" class="delete_icon tooltip_html">删除</a></li>
        </ul>
      </div>
      <div class="ab_list_box" style="display: none;">
        <div class="ab_article">
          <% que[:branch_questions].each do |bq| %>
              <div class="questions_item">
                <div class="q_topic">
                  <div class="speedQuestions">
                    <ul>
                      <li><a href="#" class="<%= bq[:answer] == "true" ? "true" : "false"  %>">T</a></li>
                      <li class="qt_text"><p><%= bq.content  %></p><input name="" type="text"></li>
                      <li><a href="#" class="<%= bq[:answer] == "true" ? "false" : "true"  %>">F</a></li>
                    </ul>
                  </div>
                </div>
                <div class="qt_icon"><a href="#" class="tag tooltip_html">标签</a></div>
                <% if @branch_tags[bq.id].present? %>
                    <div class="tag_ul">
                      <ul>
                        <% @branch_tags[bq.id].each do |tag| %>
                            <li><p><%= tag.name %></p><a href="#" class="x">X</a></li>
                        <% end %>
                      </ul>
                    </div>
                <% end %>
              </div>
          <% end %>
        </div>
      </div>
    </div>
<% end %>