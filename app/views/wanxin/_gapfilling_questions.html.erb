<script>
    ondblclick(".qt_text p",".qt_text input");
</script>

<%index=0%>
<div class='gapFilling_questions'>
  <div class='gapFilling_questions_body'>
    <span class='gapFilling_numb'><%=index+=1%></span>
    <div class='gq_article'>
      <div class='gq_article_title'><div class='qt_text'><p> </p><input name='' type='text'/></div></div>
      <ul>
        <li><input type='radio' name='radio_<%=index%>'/><div class='qt_text'><p> </p><input name='' type='text' /></div></li>
        <li><input type='radio' name='radio_<%=index%>'/><div class='qt_text'><p> </p><input name='' type='text'/></div></li>
        <li><input type='radio' name='radio_<%=index%>'/><div class='qt_text'><p> </p><input name='' type='text'/></div></li>
        <li><input type='radio' name='radio_<%=index%>'/><div class='qt_text'><p> </p><input name='' type='text'/></div></li>
      </ul>
    </div>
  </div>
  <div class='qt_icon'>
    <a href='#' class='save tooltip_html'>保存</a>
    <a href='#' class='tag tooltip_html'>标签</a>
  </div>
  <div class='tag_ul'>
    <ul>
      <li><p>不定冠词</p><a href='#' class='x'>X</a></li>
    </ul>
  </div>
</div>
<script>
  $(document).ready(function(){
    $('input[type=checkbox], input[type=radio]').iCheck({
      checkboxClass: 'icheckbox_square-aero',
      radioClass: 'iradio_square-aero',
      increaseArea: '20%' // optional
    });
  });
</script>